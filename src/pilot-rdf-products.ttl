# ==========================================
# PILOT PRODUCT 1: Broadband 500Mbps
# ==========================================

<https://intent-platform.example.com/product/PROD-BB-500>
    a sch:Product, tmf620:ProductOffering ;

    # Basic Information (Schema.org)
    sch:name "Broadband 500Mbps"@en ;
    sch:description "High-speed broadband with 500Mbps download and 50Mbps upload"@en ;
    sch:identifier "PROD-BB-500" ;
    sch:sku "PROD-BB-500" ;

    # TMF620 Product Offering Properties
    tmf620:productOfferingId "PROD-BB-500" ;
    tmf620:productOfferingType "broadband" ;
    tmf620:lifecycleStatus "Active" ;
    tmf620:version "1.0" ;
    tmf620:validFor [
        a tmf620:TimePeriod ;
        tmf620:startDateTime "2025-01-01T00:00:00Z"^^xsd:dateTime ;
        tmf620:endDateTime "2026-12-31T23:59:59Z"^^xsd:dateTime
    ] ;

    # Pricing Information (Schema.org)
    sch:offers [
        a sch:Offer ;
        sch:price "39.99"^^xsd:decimal ;
        sch:priceCurrency "EUR" ;
        sch:priceValidUntil "2026-12-31"^^xsd:date ;
        sch:availability sch:InStock ;
        sch:eligibleRegion "IE" ; # Ireland
        sch:billingPeriod "P1M" ; # Monthly
    ] ;

    # Technical Specifications
    tmf620:productSpecification [
        a tmf620:ProductSpecification ;
        sch:name "Broadband Specification"@en ;
        tmf620:characteristic [
            a tmf620:ProductSpecCharacteristic ;
            sch:name "Download Speed"@en ;
            sch:value "500"^^xsd:integer ;
            sch:unitCode "Mbps"
        ] , [
            a tmf620:ProductSpecCharacteristic ;
            sch:name "Upload Speed"@en ;
            sch:value "50"^^xsd:integer ;
            sch:unitCode "Mbps"
        ] , [
            a tmf620:ProductSpecCharacteristic ;
            sch:name "Data Cap"@en ;
            sch:value "Unlimited"
        ] , [
            a tmf620:ProductSpecCharacteristic ;
            sch:name "Installation"@en ;
            sch:value "Professional"
        ] , [
            a tmf620:ProductSpecCharacteristic ;
            sch:name "Contract Term"@en ;
            sch:value "12"^^xsd:integer ;
            sch:unitCode "months"
        ]
    ] ;

    # Category & Classification (SKOS)
    sch:category [
        a skos:Concept ;
        skos:prefLabel "Broadband"@en ;
        skos:inScheme <https://intent-platform.example.com/taxonomy/products>
    ] ;

    # Metadata
    dc:created "2025-01-01T00:00:00Z"^^xsd:dateTime ;
    dc:modified "2026-01-10T00:00:00Z"^^xsd:dateTime ;
    dc:creator "Intent Platform Team" ;

    # Custom Intent Platform Properties
    intent:popularityScore "85"^^xsd:integer ;
    intent:customerSegment "residential" ;
    intent:aiRecommendationWeight "0.8"^^xsd:decimal .


