{
  "name": "business-intent-agent",
  "version": "1.0.0",
  "description": "Claude AI-powered business intent processing agent",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "audit": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix",
    "security:check": "npm audit && npm run lint",
    "prebuild": "npm run security:check"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "0.20.9",
    "@modelcontextprotocol/sdk": "0.5.0",
    "axios": "1.6.8",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.18.3",
    "express-rate-limit": "7.2.0",
    "helmet": "7.1.0",
    "pino": "8.19.0",
    "pino-pretty": "10.3.1",
    "prom-client": "15.1.0",
    "redis": "4.6.13"
  },
  "devDependencies": {
    "@types/compression": "1.8.5",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.30",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "eslint": "8.57.0",
    "jest": "29.7.0",
    "prettier": "3.2.5",
    "supertest": "6.3.4",
    "ts-jest": "29.1.2",
    "ts-node-dev": "2.0.0",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "author": "Vpnet Consulting LLC",
  "license": "PROPRIETARY"
}
