{
  "name": "business-intent-agent",
  "version": "1.0.0",
  "description": "Claude AI-powered business intent processing agent",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint '*.ts' --ignore-pattern 'dist/**' --ignore-pattern '**/*.test.ts'",
    "format": "prettier --write '*.ts' '!dist/**' '!**/*.test.ts'",
    "audit": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix",
    "security:check": "npm audit && npm run lint"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@modelcontextprotocol/sdk": "^1.25.1",
    "axios": "^1.13.2",
    "compression": "^1.8.1",
    "cors": "2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "prom-client": "^15.1.3",
    "redis": "^5.10.0"
  },
  "devDependencies": {
    "@types/compression": "1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "eslint": "^9.39.2",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node-dev": "2.0.0",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "author": "Vpnet Consulting LLC",
  "license": "PROPRIETARY"
}
