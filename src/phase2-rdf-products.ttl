# ==========================================
# UPGRADE RELATIONSHIPS
# ==========================================

# Broadband Upgrade Path
<https://intent-platform.example.com/product/PROD-BB-500>
    intent:upgradesTo <https://intent-platform.example.com/product/PROD-BB-1GB> ;
    intent:upgradePriceDifference "20.00"^^xsd:decimal .

# Mobile Upgrade Path
<https://intent-platform.example.com/product/PROD-MOB-50GB>
    intent:upgradesTo <https://intent-platform.example.com/product/PROD-MOB-UNL> ;
    intent:upgradePriceDifference "10.00"^^xsd:decimal .

# TV Upgrade Path
<https://intent-platform.example.com/product/PROD-TV-BASIC>
    intent:upgradesTo <https://intent-platform.example.com/product/PROD-TV-PREMIUM> ;
    intent:upgradePriceDifference "10.00"^^xsd:decimal .

# ==========================================
# PRODUCT COMPLEMENTARITY RELATIONSHIPS
# ==========================================

# Broadband 1Gbps complements mobile products
<https://intent-platform.example.com/product/PROD-BB-1GB>
    sch:isRelatedTo <https://intent-platform.example.com/product/PROD-MOB-50GB> ;
    sch:isRelatedTo <https://intent-platform.example.com/product/PROD-MOB-UNL> ;
    intent:complementsProduct <https://intent-platform.example.com/product/PROD-MOB-50GB> ;
    intent:complementsProduct <https://intent-platform.example.com/product/PROD-MOB-UNL> ;
    intent:complementStrength "0.90"^^xsd:decimal .

# Broadband 1Gbps complements TV products
<https://intent-platform.example.com/product/PROD-BB-1GB>
    sch:isRelatedTo <https://intent-platform.example.com/product/PROD-TV-BASIC> ;
    sch:isRelatedTo <https://intent-platform.example.com/product/PROD-TV-PREMIUM> ;
    intent:complementsProduct <https://intent-platform.example.com/product/PROD-TV-BASIC> ;
    intent:complementsProduct <https://intent-platform.example.com/product/PROD-TV-PREMIUM> ;
    intent:complementStrength "0.75"^^xsd:decimal .

# Mobile 50GB complements TV products
<https://intent-platform.example.com/product/PROD-MOB-50GB>
    sch:isRelatedTo <https://intent-platform.example.com/product/PROD-TV-BASIC> ;
    sch:isRelatedTo <https://intent-platform.example.com/product/PROD-TV-PREMIUM> ;
    intent:complementsProduct <https://intent-platform.example.com/product/PROD-TV-BASIC> ;
    intent:complementsProduct <https://intent-platform.example.com/product/PROD-TV-PREMIUM> ;
    intent:complementStrength "0.60"^^xsd:decimal .

